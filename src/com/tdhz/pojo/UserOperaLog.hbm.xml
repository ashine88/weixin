<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.tdhz.pojo">
	<!--描述类和表的关系 -->
	<class name="UserOperaLog" table="sys_user_operalog">
		<!-- 描述java对象属性和表的列明对应关系 -->
		<!-- 主键 -->
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>
		<!-- 普通属性 -->
		<property name="username" column="username" type="java.lang.String"></property>
		<property name="operadate" column="operadate" type="java.util.Date"></property>
		<property name="data" column="data" type="java.lang.String"></property>
	</class>
</hibernate-mapping>