<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.tdhz.pojo">
	<!--描述类和表的关系 -->
	<class name="Users_Role" table="sys_user_role">
		<!-- 描述java对象属性和表的列明对应关系 -->
		<!-- 主键 -->
		<id name="id" column="role_function_id" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>
		<!-- 普通属性 -->
		<property name="role_id" column="role_id" type="java.lang.Integer"></property>
		<property name="user_id" column="user_id" type="java.lang.Integer"></property>
		
		
	</class>
</hibernate-mapping>