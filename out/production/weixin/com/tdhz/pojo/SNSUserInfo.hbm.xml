<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.tdhz.pojo">
	<!-- 描述类和表的关系 -->
	<class name="SNSUserInfo" table="sys_weixinInfo">
		<!-- 描述java对象属性和表的列明对应关系 -->
		<id name="siid" column="wiid" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>		
		<property name="openId" column="openId" type="java.lang.String"></property>		
		<property name="nickname" column="nickname" type="java.lang.String"></property>		
		<property name="sex" column="sex" type="java.lang.Integer"></property>		
		<property name="country" column="country" type="java.lang.String"></property>		
		<property name="province" column="province" type="java.lang.String"></property>		
		<property name="city" column="city" type="java.lang.String"></property>	
		<property name="headImgUrl" column="headImgUrl" type="java.lang.String"></property>			
		
	</class>
</hibernate-mapping>